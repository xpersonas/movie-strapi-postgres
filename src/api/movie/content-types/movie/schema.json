{
  "kind": "collectionType",
  "collectionName": "movies",
  "info": {
    "singularName": "movie",
    "pluralName": "movies",
    "displayName": "Movie",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "content-manager": {
      "visibleFields": [
        "title",
        "release_date"
      ],
      "editRelations": []
    }
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Runtime": {
      "type": "string"
    },
    "Poster": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Release": {
      "type": "date"
    },
    "Year": {
      "type": "integer"
    },
    "Genre": {
      "type": "string"
    },
    "PlotSummary": {
      "type": "text"
    },
    "Language": {
      "type": "string"
    },
    "imdbRating": {
      "type": "decimal"
    },
    "RottenTomatoesRating": {
      "type": "integer"
    },
    "MetaCriticScore": {
      "type": "integer"
    },
    "imdbVotes": {
      "type": "integer"
    },
    "BoxOffice": {
      "type": "biginteger"
    },
    "imdbId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Rated": {
      "type": "string"
    }
  }
}
